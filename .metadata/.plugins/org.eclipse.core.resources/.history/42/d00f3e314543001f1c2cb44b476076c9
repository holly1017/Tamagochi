package controller;

import model.RandomCharacter;

public class TamagochiController {
	
	RandomCharacter rc = new RandomCharacter();

	public void setLevel(int level) {
		rc.setLevel(level);
		
	}

	public String generateRandomCharacter() {
		return rc.generateRandomCharacter();
		
	}

	public String getStatus() {
		return rc.getStatus();
		
	}

}
